{
  "name": "ka-web",
  "version": "0.0.1",
  "description": "Web client for the open source game Kenó Antigen.",
  "workspaces": [
    "stubs/"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Kantigen/ka-web"
  },
  "author": "Lacuna Expanse Corp, Kenó Antigen Community & Open Source Contributors",
  "contributors": [
    "Nathan McCallum <hey@nathan-mccallum.com> (https://github.com/1vasari)",
    "Darin McBride <dmcbride@cpan.org> (https://github.com/dmcbride)"
  ],
  "bugs": {
    "url": "https://github.com/Kantigen/ka-web/issues"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write app/**/*.ts app/**/*.tsx app/yui/*.js app/components/**/*.jsx stubs/**/*.js",
    "dev": "concurrently \"npm run serve:app\" \"npm run serve:stubs\" \"npm run serve:assets\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "vite build",
    "serve:app": "vite",
    "serve:stubs": "nodemon --exec \"node stubs/server.js\" --watch stubs",
    "serve:assets": "http-server ./Lacuna-Assets --cors --port 4000"
  },
  "homepage": "https://github.com/Kantigen/ka-web",
  "dependencies": {
    "classnames": "^2.3.1",
    "create-react-class": "^15.6.3",
    "fomantic-ui": "^2.8.8",
    "formik": "^2.2.9",
    "jquery": "^3.5.0",
    "lodash": "^4.17.15",
    "mobx": "^6.3.7",
    "mobx-react": "^7.2.1",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^17.x.x",
    "react-dom": "^17.x.x",
    "react-draggable": "^4.0.3",
    "react-tabs": "^4.0.0",
    "react-tooltip": "^4.x.x",
    "validator": "^13.0.0",
    "vex-js": "^4.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/jquery": "^3.5.14",
    "@types/lodash": "^4.14.182",
    "@types/moment": "^2.13.0",
    "@types/node": "^17.0.24",
    "@types/react": "^17.x.x",
    "@types/react-dom": "^17.x.x",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "http-server": "^14.1.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.0.0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4",
    "vite": "^2.9.9"
  }
}
