{
  "name": "ka-web",
  "version": "0.0.1",
  "description": "Web client for the open source game Kenó Antigen.",
  "type": "module",
  "main": "app/load.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Kantigen/ka-web"
  },
  "author": "Lacuna Expanse Corp, Kenó Antigen Community & Open Source Contributors",
  "contributors": [
    "Nathan McCallum <hey@nathan-mccallum.com> (https://github.com/1vasari)",
    "Darin McBride <dmcbride@cpan.org> (https://github.com/dmcbride)"
  ],
  "bugs": {
    "url": "https://github.com/Kantigen/ka-web/issues"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write app/**/*.ts app/**/*.tsx app/yui/*.js app/components/**/*.jsx gulp-tasks/**/*.js stubs/**/*.js",
    "dev": "concurrently \"npm run serve:app\" \"npm run serve:stubs\"",
    "build": "gulp build",
    "clean": "gulp clean",
    "serve:app": "gulp devWithServer",
    "serve:stubs": "nodemon --exec \"node stubs/stubs.js\" --watch stubs"
  },
  "homepage": "https://github.com/Kantigen/ka-web",
  "dependencies": {
    "classnames": "^2.3.1",
    "create-react-class": "^15.6.3",
    "fingerprintjs2": "^2.1.0",
    "fomantic-ui": "^2.8.8",
    "jquery": "^3.5.0",
    "lodash": "^4.17.15",
    "mobx": "^6.3.7",
    "mobx-react": "^7.2.1",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^17.x.x",
    "react-dom": "^17.x.x",
    "react-draggable": "^4.0.3",
    "react-tabs": "^4.0.0",
    "react-tooltip": "^4.x.x",
    "validator": "^13.0.0",
    "vex-js": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jquery": "^3.5.14",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.24",
    "@types/react": "^17.x.x",
    "@types/react-dom": "^17.x.x",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "ansi-colors": "^4.1.1",
    "babel-eslint": "^10.0.3",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "browserify-css": "^0.15.0",
    "concurrently": "^7.1.0",
    "del": "^6.0.0",
    "envify": "^4.1.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "express": "^4.18.1",
    "gulp": "^4.x.x",
    "gulp-typescript": "^6.0.0-alpha.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.0.0",
    "tsify": "^5.0.4",
    "typescript": "^4.6.4",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^4.0.0"
  }
}
